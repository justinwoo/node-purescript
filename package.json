{
	"name": "purescript",
	"version": "0.12.3",
	"description": "PureScript wrapper that makes it available as a local dependency",
	"license": "ISC",
	"repository": "purescript-contrib/node-purescript",
	"author": "Shinnosuke Watanabe (http://github.com/shinnn)",
	"engines": {
		"node": ">=8.10.0",
		"npm": ">=5.6.0"
	},
	"bin": {
